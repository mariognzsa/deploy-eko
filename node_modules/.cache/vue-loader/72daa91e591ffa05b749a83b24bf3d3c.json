al.id}}</p>\n                    <div class=\"text--primary\">\n                        <v-form>\n                            <v-row>\n                                <v-switch\n                                    v-model=\"tutorialActive\"\n                                    label=\"Activar\"\n                                    v-bind=\"attrs\"\n                                    v-on=\"on\"\n                                ></v-switch>\n                                <v-tooltip right>\n                                    <template v-slot:activator=\"{ on, attrs }\">\n                                        <v-icon\n                                        class=\"px-2\"\n                                        v-bind=\"attrs\"\n                                        v-on=\"on\"\n                                        size=\"small\"\n                                        color=\"blue\"\n                                        >\n                                        mdi-help\n                                        </v-icon>\n                                    </template>\n                                    <span>Determina si el tutorial es visible para los usuarios</span>\n                                </v-tooltip>\n                            </v-row>\n                            \n                            <v-text-field\n                                v-model=\"editedTutorial.title\"\n                                counter=\"25\"\n                                hint=\"Titulo del tutorial\"\n                                label=\"Titulo\"\n                                color=\"primary\"\n                            ></v-text-field>\n                            <v-textarea\n                                v-model=\"editedTutorial.description\"\n                                label=\"Descripcion\"\n                                auto-grow\n                                rows=\"3\"\n                                row-height=\"25\"\n                                color=\"primary\"\n                            ></v-textarea>\n                            <v-select\n                                v-model=\"e7\"\n                                :items=\"etiquetas\"\n                                label=\"Select\"\n                                multiple\n                                chips\n                                hint=\"Etiquetas\"\n                                persistent-hint\n                            ></v-select>\n                        </v-form>\n                    </div>\n                </v-card-text>\n                <v-card-actions>\n                    <v-spacer></v-spacer>\n                    <v-btn\n                        class=\"mr-4\"\n                        @click=\"propModel = false\"\n                    >\n                        Cancelar\n                    </v-btn>\n                    <v-btn\n                        color=\"primary\"\n                        class=\"mr-4\"\n                        @click=\"updateTutorial\"\n                    >\n                        Guardar\n                    </v-btn>\n                </v-card-actions>\n            </v-card>\n        </v-dialog>\n    </div>\n</template>\n\n<script>\nimport { db } from \"@/main\";\n\nexport default {\n    name: 'AddCourse',\n    props: {\n        value: Boolean, \n        editedTutorial: Object\n    },\n    data () {\n        return {\n            tutorial: null,\n            etiquetas: ['Musica', 'Otros', 'Alaska']\n        }\n    },\n    methods: {\n        updateTutorial () {\n            db.collection('Tutoriales').doc(this.editedTutorial.id).set({\n                titulo: this.editedTutorial.title,\n                descripcion : this.editedTutorial.description\n            })\n            .then(function () {\n                console.log('Documento escrito satisfactoriamente');\n            })\n            .catch(function (error) {\n                console.log('error : ', error);\n            })\n        }\n    },\n    computed: {\n        propModel: {\n            get () { return this.value },\n            set (value) { this.$emit('input', value) },\n        },\n    },\n    mounted () {\n    }\n}\n</script>\n\n<style>\n\n</style>"]}]}